{
  "name": "@veksa/re-reselect",
  "version": "5.1.1-p7",
  "description": "Enhance Reselect selectors with deeper memoization and cache management",
  "main": "./dist/cjs/index.js",
  "types": "./dist/reReselect.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/reReselect.d.ts",
      "import": "./dist/reReselect.mjs",
      "default": "./dist/cjs/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "build": "yarn clean && tsup",
    "lint": "eslint",
    "test": "jest ./src",
    "clean": "rimraf dist",
    "preversion": "yarn prepare",
    "version": "git add package.json",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "react",
    "redux",
    "reselect",
    "memoize",
    "cache"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toomuchdesign/re-reselect.git"
  },
  "author": "Andrea Carraro <me@andreacarraro.it>",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "__util__"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^8.34.1",
    "@veksa/reselect": "^5.1.1-p7",
    "all-contributors-cli": "^6.26.1",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^30.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@veksa/reselect": "^5.1.1-p7"
  },
  "packageManager": "yarn@4.9.2"
}
