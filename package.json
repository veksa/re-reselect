{
  "name": "@veksa/re-reselect",
  "version": "5.1.1-p3",
  "description": "Enhance Reselect selectors with deeper memoization and cache management",
  "main": "./dist/cjs/index.js",
  "types": "./dist/reReselect.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/reReselect.d.ts",
      "import": "./dist/reReselect.mjs",
      "default": "./dist/cjs/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "build": "yarn clean && tsup",
    "lint": "eslint src",
    "test": "jest ./src",
    "clean": "rimraf dist",
    "preversion": "yarn prepare",
    "version": "git add package.json",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "react",
    "redux",
    "reselect",
    "memoize",
    "cache"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toomuchdesign/re-reselect.git"
  },
  "author": "Andrea Carraro <me@andreacarraro.it>",
  "license": "MIT",
  "peerDependencies": {
    "@veksa/reselect": "^5.1.1-p3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/eslint-plugin-tslint": "^6",
    "@typescript-eslint/parser": "^6",
    "@veksa/reselect": "^5.1.1-p3",
    "all-contributors-cli": "^6.26.1",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-typescript": "0.14.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "__util__"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ]
  },
  "packageManager": "yarn@4.7.0"
}
